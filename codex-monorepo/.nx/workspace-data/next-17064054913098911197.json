{
  "9895957872316360634": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/Blog",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/Blog/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/Blog/.next/!(cache)"
      ],
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/Blog"
      },
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/Blog"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/Blog"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "npx nx watch --projects @codex-monorepo/Blog --includeDependentProjects -- npx nx build-deps @codex-monorepo/Blog"
    }
  },
